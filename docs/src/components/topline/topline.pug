include ../nav/nav

mixin topline({ items } = {})
  if !items
    -
      items = [
        { name: "Home" },
        { name: "Playground", active: true },
        { name: "Quick start" },
      ];
  header.topline
    .container-fluid.container-fluid_size_md
      .spacer.clearfix
        h1.topline__logo Slider name
        .topline__nav
          +nav(items).nav_main
