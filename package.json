{
  "name": "slider",
  "private": true,
  "version": "1.0.0",
  "description": "Bulletproof range slider plugin with MVC architecture at the core",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alanreidt/slider.git"
  },
  "scripts": {
    "riteway": "riteway -r esm",
    "test": "parcel tests/test.html --open",
    "build": "rm -rf prod && parcel build src/Slider.js -o src/Slider.min.js -d ./prod && parcel build src/slider.scss -o src/slider.min.css -d ./prod",
    "dev": "parcel test-page/index.html --open",
    "eslint": "eslint src/",
    "create": "plop"
  },
  "keywords": [
    "range",
    "slider",
    "range-slider",
    "MVC"
  ],
  "author": "alanreidt",
  "license": "MIT",
  "browser": "prod/Slider.min.js",
  "devDependencies": {
    "bootstrap": "^4.4.1",
    "chai": "^4.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "jquery": "^3.5.0",
    "mocha": "^6.2.2",
    "parcel-bundler": "^1.12.4",
    "plop": "^2.5.3",
    "popper.js": "^1.16.1",
    "prettier": "1.19.1",
    "pug": "^2.0.4",
    "riteway": "^6.1.1",
    "sass": "^1.23.7"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.6",
    "deep-equal": "^2.0.1",
    "lodash": "^4.17.15"
  }
}
